{
  "typescript.tsdk": "./node_modules/typescript/lib",
  "editor.tabSize": 2,
  // "editor.formatOnSave": false,
  // "editor.codeActionsOnSave": {
  //   // "source.fixAll.eslint": "always",
  //   // "source.fixAll.stylelint": "true"
  // },

  "stylelint.validate": [
    "css",
    "scss",
    "less",
    "postcss"
  ],
  "npm.packageManager": "pnpm",
  "i18n-ally.localesPaths": [
    "src/locales/lang"
  ],
  "i18n-ally.enabledParsers": [
    "json"
  ],
  "i18n-ally.pathMatcher": "{locale}/{namespaces}.{ext}",
  "i18n-ally.keystyle": "flat",
  "i18n-ally.sortKeys": true,
  "i18n-ally.sourceLanguage": "en_US",
  "i18n-ally.displayLanguage": "zh_CN",
  "cSpell.ignoreWords": [
    "Antd",
    "CLOSEALL",
    "CLOSELEFT",
    "CLOSEOTHERS",
    "CLOSERIGHT",
    "Kanban",
    "Sider",
    "ahooks",
    "anticon",
    "apexcharts",
    "clsx",
    "commitlint",
    "cssinjs",
    "daygrid",
    "fullcalendar",
    "iconify",
    "jian",
    "languagedetector",
    "matchs",
    "nprogress",
    "nums",
    "partialize",
    "pinia",
    "rehype",
    "screenfull",
    "setopen",
    "shengXiao",
    "simplebar",
    "stylelint",
    "tabpane",
    "tailwindcss",
    "tanstack",
    "timegrid",
    "unocss",
    "vueuse",
    "weiShaoY",
    "zustand"
  ],
}
